<head>
<style>
  ul {
      list-style-type: none;
      margin: 0;
      padding: 0;
  }

  li {
    display: inline-flex;
  }

  a, h1, h2, h3 {
    text-decoration: none;
  }
</style>
</head>

<ul>
  <li>Wikinimous</li>
  <li><a href="articles/new">New Article</a></li>
</ul>

<header>
  <% @articles.each do |article| %>
    <h2><%= link_to article.title, article_path(article) %></h2>
    <p><%= article.content %></p>
    <% end %>
</header>

